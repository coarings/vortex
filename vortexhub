coroutine.wrap(AIMTV_fake_script)()
local function AWHJQA_fake_script() -- walkslider.updater 
	local script = Instance.new('LocalScript', walkslider)

	local UserInputService = game:GetService("UserInputService")
	local Players = game:GetService("Players") -- Get the Players service
	local slider = script.Parent
	local fill = script.Parent.fill
	local trigger = script.Parent.trigger
	
	local slideractive = false
	local minWalkSpeed = 16 -- Minimum walk speed
	local maxWalkSpeed = 500 -- Maximum walk speed
	
	-- Function to map slider value to walk speed
	function UpdateSlider(input)
		local output = math.clamp((input.Position.X - slider.AbsolutePosition.X) / slider.AbsoluteSize.X, 0, 1)
		fill.Size = UDim2.fromScale(output, 1)
	
		-- Calculate the walk speed based on the slider value
		local walkSpeed = minWalkSpeed + (maxWalkSpeed - minWalkSpeed) * output
		local player = Players.LocalPlayer
		if player and player.Character and player.Character:FindFirstChild("Humanoid") then
			player.Character.Humanoid.WalkSpeed = walkSpeed
		end
	end
	
	function activateslider()
		slideractive = true
	end
	
	trigger.MouseButton1Down:Connect(function()
		activateslider()
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if slideractive and input.UserInputType == Enum.UserInputType.MouseMovement then
			UpdateSlider(input)
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			slideractive = false
		end
	end)
	
end
coroutine.wrap(AWHJQA_fake_script)()
local function CSGS_fake_script() -- fill.LocalScript 
	local script = Instance.new('LocalScript', fill)

	local frame = script.Parent -- Reference to the Frame
	local add = 10
	wait(1)
	local k = 1
	
	while k <= 255 do
		frame.BackgroundColor3 = Color3.new(k / 255, 0 / 255, 0 / 255)
		k = k + add
		wait()
	end
	
	while true do
		k = 1
		while k <= 255 do
			frame.BackgroundColor3 = Color3.new(255 / 255, k / 255, 0 / 255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			frame.BackgroundColor3 = Color3.new(255 / 255 - k / 255, 255 / 255, 0 / 255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			frame.BackgroundColor3 = Color3.new(0 / 255, 255 / 255, k / 255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			frame.BackgroundColor3 = Color3.new(0 / 255, 255 / 255 - k / 255, 255 / 255)
			k = k + add
			wait()
		end
