		while runningRainbow do
			local nextColor = colors[colorIndex]
			local colorTween = TweenService:Create(button, tweenInfo, {TextColor3 = nextColor})
			colorTween:Play()
	
			colorTween.Completed:Wait()
			colorIndex = (colorIndex % #colors) + 1
		end
	end
	
	-- Hover effect
	button.MouseEnter:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
	
		playBounceEffect()
		playRainbowEffect()
	end)
	
	button.MouseLeave:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
		runningRainbow = false -- Stop the rainbow effect
	
		local goal = {Size = originalSize, Position = originalPosition, TextColor3 = originalTextColor}
		currentTween = createTween(goal)
		currentTween:Play()
	end)
	
