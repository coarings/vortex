local function EPRIJLI_fake_script() -- back.backscript 
	local script = Instance.new('LocalScript', back)

	--// put all of that in a mouse button click function
	script.Parent.MouseButton1Click:Connect(function()
		--// hides all tabs
		local tabs = script.Parent.Parent.Parent.Parent.thold
		for i, v in pairs(tabs:GetChildren()) do
			if v:IsA("Frame") then
				v.Visible = false
			end
		end
	
		--// shows tab u want
		script.Parent.Parent.Parent.Parent.thold.home.Visible = true
		--                                /\ PUT TAB NAME THERE	
	end)
end
coroutine.wrap(EPRIJLI_fake_script)()
local function BUZIO_fake_script() -- TextLabel_6.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_6)

	local text = script.Parent
	local add = 10
	wait(1)
	local k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(k/255,0/255,0/255)
		k = k + add
		wait()
	end
	while true do
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,k/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255,k/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(k/255,0/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
			k = k + add
			wait()
		end
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
			k = k + add
			wait()
		end
	end
end
coroutine.wrap(BUZIO_fake_script)()
local function LYZEPB_fake_script() -- jumpbox.jumpscript 
	local script = Instance.new('LocalScript', jumpbox)

	local plr = game.Players.LocalPlayer
	local char = plr.Character
	local hum = char:WaitForChild("Humanoid")
	
	
	script.Parent.Changed:Connect(function()
		hum.JumpPower = script.Parent.Text
	end)
end
coroutine.wrap(LYZEPB_fake_script)()
local function TTXYL_fake_script() -- walkslider.updater 
	local script = Instance.new('LocalScript', walkslider)

	local UserInputService = game:GetService("UserInputService")
	local slider = script.Parent
	local fill = script.Parent.fill
	local trigger = script.Parent.trigger
	
	local slideractive = false
	
	function UpdateSlider(input)
		local output = math.clamp((input.Position.X - slider.AbsolutePosition.X) / slider.AbsoluteSize.X, 0, 1)
		fill.Size = UDim2.fromScale(output, 1)
	end
	
	function activateslider()
		slideractive = true
	end
	
	trigger.MouseButton1Down:Connect(function()
		activateslider()
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if slideractive and input.UserInputType == Enum.UserInputType.MouseMovement then
			UpdateSlider(input)
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			slideractive = false
		end
	end)
	
end
coroutine.wrap(TTXYL_fake_script)()
local function QERWGCE_fake_script() -- back_2.backscript 
	local script = Instance.new('LocalScript', back_2)

	--// put all of that in a mouse button click function
	script.Parent.MouseButton1Click:Connect(function()
		--// hides all tabs
		local tabs = script.Parent.Parent.Parent.Parent.thold
		for i, v in pairs(tabs:GetChildren()) do
			if v:IsA("Frame") then
				v.Visible = false
			end
		end
	
		--// shows tab u want
		script.Parent.Parent.Parent.Parent.thold.home.Visible = true
		--                                /\ PUT TAB NAME THERE	
	end)
end
coroutine.wrap(QERWGCE_fake_script)()
local function IDVVP_fake_script() -- TextLabel_7.LocalScript 
	local script = Instance.new('LocalScript', TextLabel_7)

	local text = script.Parent
	local add = 10
	wait(1)
	local k = 1
	while k <= 255 do
		text.TextColor3 = Color3.new(k/255,0/255,0/255)
		k = k + add
		wait()
	end
	while true do
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,k/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,255/255,0/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255,k/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(0/255,255/255 - k/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(k/255,0/255,255/255)
			k = k + add
			wait()
		end
		k = 1
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255,0/255,255/255 - k/255)
			k = k + add
			wait()
		end
		while k <= 255 do
			text.TextColor3 = Color3.new(255/255 - k/255,0/255,0/255)
			k = k + add
			wait()
		end
	end
end
