	-- Store the original size, position, and text color
	local originalSize = button.Size
	local originalPosition = button.Position
	local originalTextColor = button.TextColor3
	
	-- Define tween information for the bounce effect
	local tweenInfo = TweenInfo.new(
		0.1, -- Time
		Enum.EasingStyle.Quad, -- EasingStyle
		Enum.EasingDirection.Out -- EasingDirection
	)
	
	-- Define the size increase and position offset for the bounce effect
	local hoverSizeIncrease = UDim2.new(0, 5, 0, 5)
	local goalSizeIncrease = UDim2.new(0, 2, 0, 2) -- Slight bounce increase
	local goalPositionOffset = UDim2.new(0, -1, 0, -1) -- Slight position offset
	
	-- Variable to keep track of current tween
	local currentTween
	local runningRainbow = false
	
	-- Create tween function
	local function createTween(goal)
		return TweenService:Create(button, tweenInfo, goal)
	end
	
	-- Function to play bounce effect
	local function playBounceEffect()
		if currentTween then
			currentTween:Cancel()
		end
	
		local goal = {Size = originalSize + hoverSizeIncrease, Position = originalPosition + goalPositionOffset}
		currentTween = createTween(goal)
		currentTween:Play()
		currentTween.Completed:Connect(function()
			local goal = {Size = originalSize + goalSizeIncrease, Position = originalPosition + goalPositionOffset}
			currentTween = createTween(goal)
			currentTween:Play()
		end)
	end
	
	-- Function to play rainbow effect
	local function playRainbowEffect()
		local colors = {
			Color3.fromRGB(255, 0, 0),    -- Red
			Color3.fromRGB(255, 127, 0),  -- Orange
			Color3.fromRGB(255, 255, 0),  -- Yellow
			Color3.fromRGB(0, 255, 0),    -- Green
			Color3.fromRGB(0, 0, 255),    -- Blue
			Color3.fromRGB(75, 0, 130),   -- Indigo
			Color3.fromRGB(148, 0, 211)   -- Violet
		}
	
		local colorIndex = 1
		runningRainbow = true
	
		while runningRainbow do
			local nextColor = colors[colorIndex]
			local colorTween = TweenService:Create(button, tweenInfo, {TextColor3 = nextColor})
			colorTween:Play()
	
			colorTween.Completed:Wait()
			colorIndex = (colorIndex % #colors) + 1
		end
	end
	
	-- Hover effect
	button.MouseEnter:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
	
		playBounceEffect()
		playRainbowEffect()
	end)
	
	button.MouseLeave:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
		runningRainbow = false -- Stop the rainbow effect
	
		local goal = {Size = originalSize, Position = originalPosition, TextColor3 = originalTextColor}
		currentTween = createTween(goal)
		currentTween:Play()
	end)
	
end
coroutine.wrap(XWVF_fake_script)()
local function TMFSN_fake_script() -- mainhold.drags 
	local script = Instance.new('LocalScript', mainhold)

	spawn(function()
		local UIS = game:GetService("UserInputService")
		function normal_dragify(Frame)
			ndragToggle = nil
			local ndragSpeed = 0.125
			ndragInput = nil
			ndragStart = nil
			local ndragPos = nil
			function nupdateInput(input)
				local nDelta = input.Position - ndragStart
				local Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + nDelta.X, startPos.Y.Scale, startPos.Y.Offset + nDelta.Y)
				game:GetService("TweenService"):Create(Frame, TweenInfo.new(ndragSpeed), {Position = Position}):Play()
			end
			Frame.InputBegan:Connect(function(input)
				if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) and UIS:GetFocusedTextBox() == nil then
					ndragToggle = true
					ndragStart = input.Position
					startPos = Frame.Position
					input.Changed:Connect(function()
						if input.UserInputState == Enum.UserInputState.End then
							ndragToggle = false
						end
					end)
				end
			end)
			Frame.InputChanged:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
					ndragInput = input
				end
			end)
			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if input == ndragInput and ndragToggle then
					nupdateInput(input)
				end
			end)
		end
	
		normal_dragify(script.Parent)
	end)
end
coroutine.wrap(TMFSN_fake_script)()
local function BQTIWA_fake_script() -- open.LocalScript 
	local script = Instance.new('LocalScript', open)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.mainhold.Visible = true
	end)
end
coroutine.wrap(BQTIWA_fake_script)()
local function VEWVY_fake_script() -- open.color 
	local script = Instance.new('LocalScript', open)

	-- LocalScript inside the TextButton
	
	local button = script.Parent
	local TweenService = game:GetService("TweenService")
	
	-- Store the original size, position, and text color
	local originalSize = button.Size
	local originalPosition = button.Position
	local originalTextColor = button.TextColor3
	
	-- Define tween information for the bounce effect
	local tweenInfo = TweenInfo.new(
		0.1, -- Time
		Enum.EasingStyle.Quad, -- EasingStyle
		Enum.EasingDirection.Out -- EasingDirection
	)
	
	-- Define the size increase and position offset for the bounce effect
	local hoverSizeIncrease = UDim2.new(0, 5, 0, 5)
	local goalSizeIncrease = UDim2.new(0, 2, 0, 2) -- Slight bounce increase
	local goalPositionOffset = UDim2.new(0, -1, 0, -1) -- Slight position offset
	
	-- Variable to keep track of current tween
	local currentTween
	local runningRainbow = false
	
	-- Create tween function
	local function createTween(goal)
		return TweenService:Create(button, tweenInfo, goal)
	end
	
	-- Function to play bounce effect
	local function playBounceEffect()
		if currentTween then
			currentTween:Cancel()
		end
	
		local goal = {Size = originalSize + hoverSizeIncrease, Position = originalPosition + goalPositionOffset}
		currentTween = createTween(goal)
		currentTween:Play()
		currentTween.Completed:Connect(function()
			local goal = {Size = originalSize + goalSizeIncrease, Position = originalPosition + goalPositionOffset}
			currentTween = createTween(goal)
			currentTween:Play()
		end)
	end
	
	-- Function to play rainbow effect
	local function playRainbowEffect()
		local colors = {
			Color3.fromRGB(255, 0, 0),    -- Red
			Color3.fromRGB(255, 127, 0),  -- Orange
			Color3.fromRGB(255, 255, 0),  -- Yellow
			Color3.fromRGB(0, 255, 0),    -- Green
			Color3.fromRGB(0, 0, 255),    -- Blue
			Color3.fromRGB(75, 0, 130),   -- Indigo
			Color3.fromRGB(148, 0, 211)   -- Violet
		}
	
		local colorIndex = 1
		runningRainbow = true
	
		while runningRainbow do
			local nextColor = colors[colorIndex]
			local colorTween = TweenService:Create(button, tweenInfo, {TextColor3 = nextColor})
			colorTween:Play()
	
			colorTween.Completed:Wait()
			colorIndex = (colorIndex % #colors) + 1
		end
	end
	
	-- Hover effect
	button.MouseEnter:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
	
		playBounceEffect()
		playRainbowEffect()
	end)
	
	button.MouseLeave:Connect(function()
		if currentTween then
			currentTween:Cancel()
		end
		runningRainbow = false -- Stop the rainbow effect
	
		local goal = {Size = originalSize, Position = originalPosition, TextColor3 = originalTextColor}
		currentTween = createTween(goal)
		currentTween:Play()
	end)
	
end
coroutine.wrap(VEWVY_fake_script)()
