local function UQPD_fake_script() -- jumpslider.jumpupdater 
	local script = Instance.new('LocalScript', jumpslider)

	local UserInputService = game:GetService("UserInputService")
	local Players = game:GetService("Players") -- Get the Players service
	local slider = script.Parent
	local fill2 = script.Parent.fill2 -- Reference to fill2
	local trigger2 = script.Parent.trigger2 -- Reference to trigger2
	
	local slideractive = false
	local minJumpPower = 50 -- Minimum jump power
	local maxJumpPower = 200 -- Maximum jump power
	
	-- Function to map slider value to jump power
	function UpdateSlider(input)
		local output = math.clamp((input.Position.X - slider.AbsolutePosition.X) / slider.AbsoluteSize.X, 0, 1)
		fill2.Size = UDim2.fromScale(output, 1) -- Update fill2 size
	
		-- Calculate the jump power based on the slider value
		local jumpPower = minJumpPower + (maxJumpPower - minJumpPower) * output
		local player = Players.LocalPlayer
		if player and player.Character and player.Character:FindFirstChild("Humanoid") then
			player.Character.Humanoid.JumpPower = jumpPower
		end
	end
	
	function activateslider()
		slideractive = true
	end
	
	trigger2.MouseButton1Down:Connect(function()
		activateslider()
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if slideractive and input.UserInputType == Enum.UserInputType.MouseMovement then
			UpdateSlider(input)
		end
	end)
	
	UserInputService.InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			slideractive = false
		end
	end)
	
end
