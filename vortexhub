local function VPSKKPP_fake_script() -- funbutton.LocalScript 
	local script = Instance.new('LocalScript', funbutton)

	--// put all of that in a mouse button click function
	script.Parent.MouseButton1Click:Connect(function()
		--// hides all tabs
		local tabs = script.Parent.Parent.Parent.Parent.thold
		for i, v in pairs(tabs:GetChildren()) do
			if v:IsA("Frame") then
				v.Visible = false
			end
		end
	
		--// shows tab u want
		script.Parent.Parent.Parent.Parent.thold.fun.Visible = true
		--                                /\ PUT TAB NAME THERE	
	end)
end
